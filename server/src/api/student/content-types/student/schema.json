{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "experience": {
      "displayName": "Experience",
      "type": "component",
      "repeatable": true,
      "component": "student.experience",
      "max": 5
    },
    "qualification": {
      "displayName": "Qualification",
      "type": "component",
      "repeatable": true,
      "component": "student.qualification"
    },
    "Skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill"
    },
    "FirstName": {
      "type": "string",
      "required": true
    },
    "LastName": {
      "type": "string"
    },
    "Contacts": {
      "type": "component",
      "repeatable": true,
      "component": "contact.phone"
    },
    "DOB": {
      "type": "date"
    },
    "Gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other",
        "Unspecified"
      ],
      "default": "Unspecified"
    },
    "MaritalStatus": {
      "type": "enumeration",
      "enum": [
        "Married",
        "Unmarried",
        "Divorced",
        "Unspecified"
      ],
      "default": "Unspecified"
    },
    "IndustriesPreference": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::industry.industry"
    },
    "FatherName": {
      "type": "string"
    },
    "Address": {
      "type": "component",
      "repeatable": false,
      "component": "contact.address"
    },
    "Profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Documents": {
      "displayName": "Document",
      "type": "component",
      "repeatable": true,
      "component": "student.document"
    },
    "Company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "SalaryNegotiation": {
      "type": "integer"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Hired",
        "Rejected",
        "In Probation",
        "In Process",
        "Interview",
        "Laid Off"
      ],
      "default": "In Process",
      "required": true
    },
    "DateOfHiring": {
      "type": "date"
    },
    "Payment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment.payment",
      "mappedBy": "Candidate"
    },
    "CandidateType": {
      "type": "enumeration",
      "enum": [
        "White Collar",
        "Labour",
        "Blue Collar"
      ],
      "default": "White Collar"
    },
    "Email": {
      "type": "string"
    }
  }
}
